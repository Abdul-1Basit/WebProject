@model IEnumerable<TempConsELearning.Models.Lecture>
<link href="https://fonts.googleapis.com/icon?family=Material+Icons" rel="stylesheet">

<script src="~/Content/MaterialiseCss/materialize.js"></script>
<script src="~/Content/MaterialiseCss/materialize.min.js"></script>
<link href="~/Content/MaterialiseCss/materialize.css" rel="stylesheet" />
<link href="~/Content/MaterialiseCss/materialize.min.css" rel="stylesheet" />
<script src="~/scripts/jquery-3.3.1.min.js"></script>

<script>
    $(document).ready(function () {
        //$('.collapsible').co();
    });
</script>
<h6>Lectures</h6>
<ul class="collapsible">
    @foreach (var item in Model)
    {
        <li>
            <div class="collapsible-header">
                <i class="material-icons">filter_drama</i>
                @item.LectureNo
                @item.LectureTitle
            </div>
            <div class="collapsible-body">
                <p>
                    @item.LectureDescription
                    <br />
                    @item.VideoTitle <br />
                    <video controls width="300px" height="300px">
                        <source src="@Url.Content(item.VideoLink)" />
                    </video>
                    <br />
                    @item.VideoDesription
                </p>
            </div>
        </li>
    }
</ul>